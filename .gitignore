# Ignore node_modules for both frontend and backend
/frontend/node_modules/
/backend/node_modules/

# Build output for frontend (React)
frontend/build/
frontend/dist/

# Build output for backend (if using any build tools like Babel, TypeScript, etc.)
backend/dist/
backend/build/

# Logs directory for the backend
/backend/logs/

# Environment variables for both frontend and backend
frontend/.env
backend/.env
frontend/.env.local
backend/.env.local
frontend/.env.*.local
backend/.env.*.local

# Operating system files
.DS_Store
Thumbs.db

# Log files for both frontend and backend
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore coverage folder (from tests)
frontend/coverage/
backend/coverage/

# Ignore IDE/editor config files
.vscode/
.idea/
*.sublime-workspace
*.sublime-project

# Ignore backup or temporary files
*.log
*.bak
*.swp
*.tmp

# Ignore dependency directories
/.pnp
.pnp.js

# Ignore Next.js build output (if applicable in the frontend)
frontend/.next/
